@model PocketRoofer.Models.EstimateViewModel

@using PocketRoofer.Models;


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SaveEstimate</title>
</head>
<body>
    <div class="jumbotron">
        <div class="row">
            <h1>PocketRoofer</h1>
            Here is your estimate
        </div>
    </div>
    <div class="row">
        <div class="form-group">
            @Html.LabelFor(m => m.address, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.address, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Email, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.bundle, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.bundle, new { @class = "form-control" })
            </div>
        </div>
    </div>
</body>
</html>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyDqnTzpohV4nNuiv-p8T6--y51y4tK52rc" ></script>
    <div id="map" style="width: 500px; height: 500px"></div>
<script>
    function initialize()
    {
     
        var map = new google.maps.Map(document.getElementById('map'),
        {
            zoom: MapModel.Zoom,
            center: {lat, lng},
            mapTypeId: google.maps.MapTypeId.SATALITE
        });

    }
    google.maps.event.addDomListener(window, 'load', initialize);
</script>
